<% if changes.empty? %>
	<%= t('changes.empty')%>
<% else %>
	<% for change in changes %>
		<%= render :partial => "changes/show", :locals => {:change => change} %>
		<br/>
		<%= render :partial => "changes/status", :locals => {:change => change} %>
		<% if change.status == 'sent' %>
			<%= render :partial => "changes/chart", :locals => {:change => change} %>
		<% end %>
		<p>
	<% end %>
<% end %>
